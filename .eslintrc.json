{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-mixed-spaces-and-tabs": "warn",
        "spaced-comment": "warn",
        "no-trailing-spaces": "warn" 
    }
}
