<?xml version="1.0" encoding="UTF-8"?><CommandWorkflows>
  <debug>
    <businessRules>
      <rules>
        <arrayParsing>
          <commandArrayParsing>
            <Workflow Name="commandArrayParsingEnable" Value="changeConfigSetting debugSetting.framework.businessRules.rules.arrayParsing.commandArrayParsing true"/>
            <Workflow Name="commandArrayParsingDisable" Value="changeConfigSetting debugSetting.framework.businessRules.rules.arrayParsing.commandArrayParsing false"/>
            <Workflow Name="solveLehmerCodeEnable" Value="changeConfigSetting debugSetting.framework.businessRules.rules.arrayParsing.commandArrayParsing.solveLehmerCode true"/>
            <Workflow Name="solveLehmerCodeDisable" Value="changeConfigSetting debugSetting.framework.businessRules.rules.arrayParsing.commandArrayParsing.solveLehmerCode false"/>
