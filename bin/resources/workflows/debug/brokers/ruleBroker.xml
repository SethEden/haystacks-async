        <Workflow Name="addClientRulesEnable" Value="changeConfigSetting debugSetting.framework.brokers.ruleBroker.addClientRules true"/>
        <Workflow Name="addClientRulesDisable" Value="changeConfigSetting debugSetting.framework.brokers.ruleBroker.addClientRules false"/>
      </ruleBroker>
    </brokers>
  </debug>
</CommandWorkflows>
