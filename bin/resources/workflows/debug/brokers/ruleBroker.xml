<?xml version="1.0" encoding="UTF-8"?><CommandWorkflows>
  <debug>
    <brokers>
      <ruleBroker>
        <Workflow Name="ruleBrokerEnable" Value="changeConfigSetting debugSetting.framework.brokers.ruleBroker true"/>
